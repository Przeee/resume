    {{#with footer}}
        <footer>
			<div>
				{{#with social}}
					{{> link href=email.href text=email.text}}
					{{> link href=phone.href text=phone.text}}
					{{> link href=linkedin.href text=linkedin.text blank=true noLine=true}}
				{{/with}}
			</div>
			<br>
			<div>
				<em>Copyright by {{../header.name.firstname}} {{../header.name.lastname}}</em>
			</div>
			<br>
			<div>
				{{../header.address}}
			</div>


        </footer>
    {{/with}}