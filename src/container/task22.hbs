<main>
    <section>
        <div>
            {{#with page_header}}
                <h1 class="text-center">{{title}}</h1>
                <p class="text-center text-capitalize">{{description}}</p>
            {{/with}}
        </div>
    </section>
    <section>
        {{#with section}}
            <div class="card">
                <div class="card-body">
                    {{#with about_us}}
                        <div class="display-6 text-end">{{title_list.main}}</div>
                        <br>
                        <h2 class="text-end">{{title_list.sub}}</h2><br>
                        <button class="btn btn-outline-info">{{button.text}}</button>
                        <br>
                    {{/with}}
                </div>
            </div>
            <br>
                <div class="card">
                    <div class="card-body">
                    {{#with advantage}}
                            <div class="display-5">{{title}}</div>
                            <br>
                            <div class="card">
                                <div class="card-body">
                                    {{#with efficiency}}
                                        <div class="display-4">{{title}}</div>
                                        <br>
                                        <h6>{{sub_title}}</h6>
                                        <p>{{description}}</p>
                                        <button class="btn btn-outline-warning">{{button}}</button>
                                    {{/with}}
                                </div>
                            </div>
                            <br>
                            <div class="card">
                                <div class="card-body">
                                    {{#with innovation}}
                                        <div class="display-4 text-center">{{title}}</div>
                                        <br>
                                        <h6 class="text-center">{{sub_title}}</h6>
                                        <p class="text-center">{{description}}</p>
                                        <button class="btn btn-outline-warning">{{button}}</button>
                                    {{/with}}
                                </div>
                            </div>
                            <br>
                            <div class="card">
                                <div class="card-body">
                                    {{#with collaboration}}
                                        <div class="display-4 text-end">{{title}}</div>
                                        <br>
                                        <h6 class="text-end">{{sub_title}}</h6>
                                        <p class="text-end">{{description}}</p>
                                        <button class="btn btn-outline-warning">{{button}}</button>
                                    {{/with}}
                                </div>
                            </div>
                    </div>
                </div>
                {{/with}}
                <br>
                <div class="card">
                    <div class="card-body">
                        {{#with agreement}}
                            <div class="display-5">{{title}}</div>
                            <br>
                            <div class="card">
                                <div class="card-body">
                                    {{#with info}}
                                        <h5>{{sub_title}}</h5>
                                        <br>
                                        <div class="card">
                                            <div class="card-body">
                                                {{#with detail}}
                                                    <p>{{text}}</p>
                                                    <br>
                                                    <button class="btn btn-success">{{button}}</button>
                                                {{/with}}
                                            </div>
                                        </div>
                                    {{/with}}
                                </div>
                            </div>
                        {{/with}}
                    </div>
                </div>
        {{/with}}
    </section>
</main>